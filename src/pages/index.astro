---
import Layout from '@/layouts/Layout.astro';
import Hero from '@/components/Hero.astro';
const preloadImages = [
	{
		href:"/images/index/hero.webp",
		as: "image",
		type: "image/webp",
	}
];
---

<Layout
	title="Vodem - Obras Industriales"
	canonical="https://vodem.com.ar"
	metaTitle="Vodem - Obras Industriales, Urbanizaciones y Pisos de Hormigón"
	metaDescription="En Vodem realizamos urbanizaciones para el crecimiento de las comunidades, obras industriales de calidad y durabilidad y pisos de hormigón de alto impacto."
	metaImg="https://vodem.com.ar/images/index/hero.webp"
	preLoad={preloadImages}
>
	<Hero />
</Layout>
