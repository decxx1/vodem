---
import Layout from '@/layouts/Layout.astro';
import Hero from '@/components/Hero.astro';
const preloadImages = [
	{
		href:"/images/index/hero.webp",
		as: "image",
		type: "image/webp",
	}
];
---

<Layout
	title="Vodem - Obras Industriales"
	canonical="https://vodem.com.ar"
	metaTitle="Vodem - Obras Industriales"
	metaDescription=""
	metaImg="https://vodem.com.ar/images/hero-potrerillos.jpg"
	preLoad={preloadImages}
>
	<Hero />
</Layout>
